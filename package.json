{
  "name": "companies",
  "version": "0.0.1",
  "description": "companies express html5 project template",
  "main": "start.js",
  "scripts": {
    "ngc": "ngc -p tsconfig-aot.json",
    "build": "tsc  -p tsconfig-tsc.json",
    "rollup": "rollup  -f iife -c rollup-config.js -o build/temp/bundle.es6.js",
    "es5": "tsc --target es5 --allowJs build/temp/bundle.es6.js --out build/temp/bundle.js",
    "minify": "uglifyjs build/temp/bundle.js --screw-ie8 --compress --mangle --output build/release/js/scripts.js",
    "release": "gulp release:clean && gulp release:precompile && npm run ngc && npm run build && npm run rollup && npm run es5 && npm run minify && gulp release:inject",
    "release-start": "static build/release",
    "debug": "gulp debug:inject && tsc -p tsconfig-debug.json",
    "debug-tsc": "tsc -p tsconfig-debug.json",
    "debug-watch": "tsc -w -p tsconfig-debug.json",
    "debug-watch-sass": "gulp watch:debug:sass",
    "debug-gulp": "gulp debug:inject",
    "debug-start": "static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akotlyachkov/companies.git"
  },
  "author": "akotlyachkov",
  "bugs": {
    "url": "https://github.com/akotlyachkov/companies/issues"
  },
  "homepage": "https://github.com/akotlyachkov/companies#readme",
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/compiler-cli": "^2.2.3",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "@types/node": "^6.0.51",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "helmet": "^3.2.0",
    "ng2-bootstrap": "^1.1.16",
    "pug": "^2.0.0-beta6",

    "rxjs": "5.0.2",
    "rxjs-es": "5.0.0-beta.12",
    "serve-favicon": "^2.3.2",
    "systemjs": "^0.19.41",

    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.1",
    "@types/node": "^6.0.54",
    "@types/core-js": "^0.9.35",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-inject": "^4.2.0",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "gulp-series": "^1.0.2",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-typescript": "^3.1.3",
    "gulp-uglify": "^2.0.0",
    "rollup": "^0.38.0",
    "rollup-plugin-commonjs": "^6.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "typescript": "^2.1.4"
  }
}
